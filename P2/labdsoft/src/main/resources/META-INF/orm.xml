        <sequence-generator name="generator" sequence-name="dom_hla_transplant_recipient_seq" allocation-size="1" />
    </entity>
    <entity class="com.pt.isep.labdsoft.entity.domain.TransplantDonor" name="TransplantDonor" access="FIELD">
        <sequence-generator name="generator" sequence-name="dom_transplant_donor_seq" allocation-size="1" />
