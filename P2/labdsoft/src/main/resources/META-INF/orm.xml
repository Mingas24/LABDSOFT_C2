    <entity class="com.pt.isep.labdsoft.entity.domain.DonorAnalysisRequest" name="DonorAnalysisRequest" access="FIELD">
        <sequence-generator name="generator" sequence-name="dom_donor_analysis_request_seq" allocation-size="1" />
    </entity>
    <entity class="com.pt.isep.labdsoft.entity.domain.RecipientAnalysisRequest" name="RecipientAnalysisRequest" access="FIELD">
        <sequence-generator name="generator" sequence-name="dom_recipient_analysis_request_seq" allocation-size="1" />
    </entity>

    <!--
    Para mapear sequências da DB aquando de um novo registo (a sequência deve ser criada nos scripts da DB com o mesmo nome do "sequence_name")
    (é normal sequence-name aparecer a vermelho)
    -->
</entity-mappings>